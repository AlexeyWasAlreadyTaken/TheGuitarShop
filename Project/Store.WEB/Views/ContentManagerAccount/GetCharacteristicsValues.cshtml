@model IEnumerable<Store.WEB.Models.CharValueVM>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CharacteristicId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IntVal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FloatVal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StrVal)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CharacteristicId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IntVal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FloatVal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StrVal)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
