@model IEnumerable<Store.WEB.Models.CharacteristicVM>

@{
    ViewBag.Title = "CreateCategoryCharacteristic";
}

<h2>CreateCategoryCharacteristic</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("ADD", "AddCategoryCharacteristic", "ContentManagerAccount", new { categoryID = Request.Params["categoryID"] , charID = item.id },null) 
        </td>
    </tr>
}

</table>
