@model IEnumerable<Store.WEB.Models.CategoryVM>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CategoryName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryName)
            </td>
            <td>
                @Html.ActionLink("GO TO", "SubCategories", "Catalog", new { id = item.CategoryId }, null)
            </td>
        </tr>
    }

</table>
